{
  "window": [
    {
      "name": "groupVideo",
      "layoutType": "QGridLayout",
      "position": [0, 1],
      "ColumnStretch": 0,
      "RowStretch": 0,
      "Align": "AlignRight",
      "children": [
        {
          "name": "video",
          "type": "label",
          "position": [0, 0],
          "ColumnStretch": 0,
          "RowStretch": 0
        },
        {
          "name": "progressBar",
          "type": "QSlider",
          "position": [1, 0],
          "ColumnStretch": 0,
          "RowStretch": 0,
          "SliderType": "Horizontal",
          "SingleStep": 100,
          "Value": 0,
          "TickInterval": 10,
          "eventFuncName": "onProgressChange",
          "eventMousedown": "onSliderMouseDown",
          "eventMouseup": "onSliderMouseUp"
        }, {
          "name": "playButtons",
          "type": "ButtonHGroup",
          "position": [2, 0],
          "ColumnStretch": 0,
          "RowStretch": 0,
          "Elements": [
            {
              "name": "play",
              "Text": "play",
              "Icon": "icons/play-outline.svg",
              "eventFuncName": "onPlayButtonClick",
              "MaximumSize": {
                "width": 50,
                "height": 50
              }
            }
          ]
        }, {
          "name": "timeShow",
          "type": "label",
          "Text": "0:00",
          "position": [3, 0],
          "ColumnStretch": 0,
          "RowStretch": 0
        }
      ]
    }, {
      "name": "groupLeftButtons",
      "layoutType": "QVBoxLayout",
      "position": [0, 0],
      "ColumnStretch": 4,
      "RowStretch": 3,
      "Align": "AlignTop",
      "children": [
        {
          "name": "leftButtons",
          "type": "ButtonVGroup",
          "position": [0, 0],
          "ColumnStretch": 0,
          "RowStretch": 0,
          "Align": "AlignTop",
          "Elements": [
            {
              "name": "play1",
              "Text": "play",
              "eventFuncName": "onplay"
            },
            {
              "name": "buttonOpenVideo",
              "Text": "打开视频",
              "eventFuncName": "openFile"
            }
          ]
        }
      ]
    }
  ]
}
